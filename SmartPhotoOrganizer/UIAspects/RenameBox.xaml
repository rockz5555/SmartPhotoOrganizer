<Window x:Class="SmartPhotoOrganizer.UIAspects.RenameBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Rename Image" Height="114" Width="412" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" ShowInTaskbar="False">
    <Grid>
        <TextBox Height="23" Margin="16,14,15,0" Name="RenameText" VerticalAlignment="Top" />
        <Button Height="23" HorizontalAlignment="Left" Margin="16,0,0,6" Name="AutoName" VerticalAlignment="Bottom" Width="141" Click="AutoNameClick">Auto re-name from tags</Button>
        <Button Height="23" Margin="0,0,93.2,6" Name="RenameOk" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" IsDefault="True" Click="RenameClick">Rename</Button>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,12,6" Name="RenameCancel" VerticalAlignment="Bottom" Width="75" IsCancel="True">Cancel</Button>
    </Grid>
</Window>